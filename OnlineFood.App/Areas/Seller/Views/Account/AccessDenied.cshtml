    <style>
        .container-access-denied {
            width: 100%;
            min-height: 100vh;
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
            align-items: center;
            padding: 15px;
            background-repeat: no-repeat;
            background-position: center;
            background-size: cover;
            position: relative;
            z-index: 1;
        }

        .page-background {
            background-image: url("/image/tomato.png");
        }

        .container-access-denied::before {
            content: "";
            display: block;
            position: absolute;
            z-index: -1;
            width: 100%;
            height: 100%;
            top: 0;
            right: 0;
            background-color: rgba(255, 255, 255, 0.7);
        }

        .wrap-login100 {
            width: 500px;
            border-radius: 30px;
            overflow: hidden;
            padding: 55px 55px 37px 55px;
            background: -webkit-linear-gradient(top, #A6CCA2, #4f7d55);
        }

        .login100-form-logo {
            font-size: 60px;
            color: #333333;
            display: flex;
            justify-content: center;
            align-items: center;
            width: 120px;
            height: 120px;
            border-radius: 50%;
            background-color: transparent;
            margin: 0 auto;
        }

            .login100-form-logo img {
                width: 100px;
                border-radius: 50%;
            }

        .form404-title {
            font-size: 50px;
            color: #fff;
            line-height: 1.2;
            text-align: center;
            display: block;
            padding-top: 30px 10px;
        }

        .text-accessdenied {
            padding: 55px;
            font-size: 2rem;
            font-weight: bold;
        }
    </style>
</head>

