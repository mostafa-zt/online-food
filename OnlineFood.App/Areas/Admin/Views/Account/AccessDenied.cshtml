@model AccessDeniedViewModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="fa">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title> عدم دسترسی | مدیریت سفارش آنلاین غذا نت فودی</title>
    <link rel="icon" href="~/favicon.ico" type="image/x-icon">
    <environment include="Development">
        <!-- Plugins Core Css -->
        <link href="~/css/admin/app.css" rel="stylesheet" />
        <!-- Custom Css -->
        <link href="~/css/admin/style.css" rel="stylesheet" />
        <!-- theme green -->
        <link href="~/css/admin/theme-green.css" rel="stylesheet" />
        <!-- farsi fonts and icons -->
        <link href="~/css/fonts-fa.css" rel="stylesheet" />
        <link href="~/css/fontawesome.css" rel="stylesheet" />
        <link href="~/css/im-icon.css" rel="stylesheet" />
        <link href="~/css/material-icon.css" rel="stylesheet" />
        <link href="~/css/glyphicon.css" rel="stylesheet" />
        <link href="~/css/menu-category.css" rel="stylesheet" />
        <!-- customize CSS Style -->
        <link href="~/css/admin/customize.css" rel="stylesheet" />
        <link href="~/css/validation.css" rel="stylesheet" />
        <link href="~/css/admin/extra_pages.css" rel="stylesheet" />
        <!-- jquery JS -->
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <!-- app Js-->
        <script src="~/js/admin/app.js"></script>
        <!-- semantic -->
        <link href="~/lib/semantic/semantic.css" rel="stylesheet" />
        <script src="~/lib/semantic/semantic.js"></script>
        <!-- JQuery Validate Unobtrusive -->
        <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    </environment>
    <environment exclude="Development">
        <link href="~/css/admin/adminmain.min.css" rel="stylesheet" />
        <!-- icons and fonts -->
        <link href="~/css/OnlineFood.min.css" rel="stylesheet"/>
        <link href="~/css/validation.min.css" rel="stylesheet" />
        <link href="~/css/admin/extra_pages.min.css" rel="stylesheet" />
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/js/admin/app.min.js"></script>
        <!-- semantic -->
        <link href="~/lib/semantic/semantic.min.css" rel="stylesheet" />
        <script src="~/lib/semantic/semantic.min.js"></script>
        <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    </environment>
</head>
<body class="login-page rtl">
    <div class="limiter">
        <div class="container-login100 page-background">
            <div class="wrap-login100">
                <form class="login100-form validate-form">
                    <span class="login100-form-logo">
                        <img src="~/image/logo.png" />
                    </span>
                    <span class="form404-title p-b-34 p-t-27">
                        عدم دسترسی
                    </span>
                    <div class="text-center">
                        <p class="txt1 p-b-20">
                            شما به این صفحه دسترسی ندارید!
                        </p>
                    </div>
                    <div class="container-login100-form-btn">
                        <a class="btn btn-outline-success btn-border-radius" href="/admin/account/login?ReturnUrl=@Model.ReturnUrl">
                            ورود به سیستم
                        </a>
                    </div>
                    @*<div class="text-center p-t-50">
                            <a class="txt1" href="sign-in.html">
                                Need Help?
                            </a>
                        </div>*@
                </form>
            </div>
        </div>
    </div>
    <environment include="Development">
        <!-- Core Js -->
        <script src="~/js/admin/admin.js"></script>
        <!-- customize JS -->
        <script src="~/js/admin/customize.js"></script>
        <script src="~/js/admin/table.min.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/admin/adminmain.min.js"></script>
        <script src="~/js/admin/table.min.js"></script>
    </environment>
    @Html.AlertNotification(TempData)
</body>
</html>